<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="ranges" alignment="TOP_RIGHT" styleClass="stack-pane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Ranges.RangesController">
   <children>
      <VBox fx:id="vBoxContainer" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" minHeight="210.0" minWidth="290.0" prefHeight="210.0" prefWidth="340.0" styleClass="UserInterfaceSection" StackPane.alignment="CENTER_LEFT">
         <children>
            <Button fx:id="filterDataButton" mnemonicParsing="false" onAction="#filterDataClicked" prefWidth="150.0" styleClass="RangeButton" stylesheets="@Ranges.css" text="Filter Data">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="sortRowsButton" mnemonicParsing="false" onAction="#sortRowsClicked" prefWidth="150.0" styleClass="RangeButton" stylesheets="@Ranges.css" text="Sort Rows">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="7.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="addRangeButton" mnemonicParsing="false" onAction="#addRangeClicked" prefHeight="25.0" prefWidth="150.0" styleClass="RangeButton" stylesheets="@Ranges.css" text="add range">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="7.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="deleteRangeButton" mnemonicParsing="false" onAction="#deleteRangeClicked" prefHeight="31.0" prefWidth="150.0" styleClass="RangeButton" stylesheets="@Ranges.css" text="delete range">
               <VBox.margin>
                  <Insets left="5.0" top="7.0" />
               </VBox.margin>
            </Button>
            <ComboBox fx:id="SystemRanges" prefWidth="150.0" promptText="Ranges" styleClass="RangeButton" stylesheets="@Ranges.css">
               <VBox.margin>
                  <Insets bottom="7.0" left="5.0" right="5.0" top="7.0" />
               </VBox.margin>
            </ComboBox>
         </children>
         <StackPane.margin>
            <Insets />
         </StackPane.margin>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../actionLine/BasicExcel.css" />
      <URL value="@../CssAdujsments/ExcelCssLayout.css" />
   </stylesheets>
</StackPane>
